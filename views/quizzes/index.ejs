<table>
  <% for (var i in quizzes) { %>        
     <tr>
        <td>
           <a href="/quizzes/<%= quizzes[i].id %>"><%= quizzes[i].question %></a>
        </td>
        <td>
          <a href="/quizzes/<%= quizzes[i].id %>/edit"><button>editar</button></a>
        </td>
        <td>
          <a href="/quizzes/<%= quizzes[i].id %>?_method=DELETE" onClicl="return confirm('Borrar: <%= quizzes[i].question%>');"><button>borrar</button>
          </a>
        </td>
     </tr> 
  <% } %>
</table>
<a href="/quizzes/new"><button type="button">Crear pregunta</button></a>
<p> Buscar pregunta: </p>
<p>
	<form method="get" action="/search">
		<input type="text" id="busqueda" name="search" value="" placeholder="Pregunta" /><p>
		<input type="submit" value="Enviar">

	</form>
</p